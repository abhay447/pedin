#include <vector>
#include <algorithm>

#include "vehicle.h"

extern vector<vehicles> allVehicles;

bool vehicleSorter(const vehicle& lhs, const vehicle& rhs)
{
  return lhs.vehiclePosition.y < rhs.vehiclePosition.y;
}

checkCollsions()
{
	std::sort(allVehicles.begin(), allVehicles.end(), &vehicleSorter);
	//for(int i=0;i<vehicle
}
